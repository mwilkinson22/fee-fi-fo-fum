{
	"name": "fee-fi-fo-fum",
	"version": "1.0.0",
	"description": "Fee Fi Fo Fum",
	"main": "index.js",
	"engines": {
		"node": "8.12.0",
		"npm": "6.4.1"
	},
	"scripts": {
		"start": "node build/bundle.js",
		"build": "NODE_ENV=production npm-run-all --parallel build:*",
		"build:client": "webpack --config webpack.client.js -p",
		"build:css": "node-sass client/scss/styles.scss --output-style compressed -o public",
		"build:server": "webpack --config webpack.server.js -p",
		"dev": "npm-run-all --parallel dev:*",
		"dev:css": "node-sass client/scss/styles.scss -o public",
		"dev:server": "nodemon --watch build --exec node build/bundle.js",
		"dev:build-server": "webpack --config webpack.server.js --watch",
		"dev:build-client": "webpack --config webpack.client.js --watch",
		"heroku-postbuild": "npm run build"
	},
	"author": "",
	"license": "ISC",
